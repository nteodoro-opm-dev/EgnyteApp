{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Egnyte": {
    "Domain": "oceanpm.egnyte.com",
    "ClientId": "[your-client-id]",
    "ClientSecret": "[your-client-secret]",
    "CallbackUrl": "https://localhost:5001/auth/callback",
    "AuthorizationEndpoint": "https://oceanpm.egnyte.com/puboauth/token",
    "TokenEndpoint": "https://oceanpm.egnyte.com/puboauth/token",
    "UserInfoEndpoint": "https://oceanpm.egnyte.com/pubapi/v1/userinfo",
    "Scopes": [
      "Egnyte.filesystem",
      "Egnyte.permission",
      "Egnyte.link",
      "Egnyte.group",
      "Egnyte.user"
    ]
  }
}